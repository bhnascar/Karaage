<% @title = "[カラアゲ] Karaage | User Control Panel" %>
<div class="container">
  <h1>Control panel</h1>
  <%= render partial: "shared/control", locals: {tab: 1} %>
  <h2>Testers who signed up</h2>
  <table class="table table-hover">
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>E-mail</th>
      <th>Organization</th>
      <th>Role</th>
    </tr>
    <% @allTesters.each do |tester| %>
      <tr>
        <td><%=tester.tester_first_name%></td>
        <td><%=tester.tester_last_name%></td>
        <td><%=tester.tester_email%></td>
        <td><%=tester.tester_organization%></td>
        <td><%=tester.tester_role%></td>
      </tr>
    <% end %>
  </table>
</div>