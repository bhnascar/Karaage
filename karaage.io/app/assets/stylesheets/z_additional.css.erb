body {
	font-family: "Open Sans", "Helvetica Neue", Helvetica, Sans-Serif;
	padding-top: 50px;
}
.jumbotron.cover-photo {
	background-image: url(<%= asset_data_uri 'cover/cover.jpg' %>);
	background-size: cover;
	padding-top: 100px;
	padding-bottom: 350px;
	padding-left: 0px;
	padding-right: 0px;
	background-position: center;
}
.dark-container {
	margin-left: 0;
	margin-right: 0;
	padding-left: 60px;
	padding-right: 60px;
	width: 100%;
	background-color: rgba(0,0,0,0.3);
	color: #fff;
	padding-top: 10px;
	padding-bottom: 10px;
}
.media-thumb {
	width: 128px;
	height: 128px;
}
.img-80 {
	max-width: 40%;
	margin: auto;
}
.navbar-translucent {
	background-color: rgba(55,55,55,0.9);
	transition-duration: 1s;
}
.navbar-translucent .navbar-nav > li > a {
  color: #bbb;
}
h1,
h2 {
	font-weight: 200;
}
h3,
h4,
h5,
h6 {
	font-weight: 300;
}
.navbar-brand {
	color: #fff !important;
}
@media (min-width: 1600px) {
  .container {
    width: 1570px;
  }
}